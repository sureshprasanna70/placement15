<p id="notice"><%= notice %></p>

<h1>Listing Academic Details</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Institution</th>
      <th>Examination authority</th>
      <th>Pass year</th>
      <th>Percentage</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @academic_details.each do |academic_detail| %>
      <tr>
        <td><%= academic_detail.course %></td>
        <td><%= academic_detail.institution %></td>
        <td><%= academic_detail.examination_authority %></td>
        <td><%= academic_detail.pass_year %></td>
        <td><%= academic_detail.percentage %></td>
        <td><%= link_to 'Show', academic_detail %></td>
        <td><%= link_to 'Edit', edit_academic_detail_path(academic_detail) %></td>
        <td><%= link_to 'Destroy', academic_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Academic detail', new_academic_detail_path %>
