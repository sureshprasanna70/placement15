<%= form_for(@semester_mark) do |f| %>
  <div>
    <%=link_to :back,:class=>"btn btn-danger" do%>
    <i class="mdi-hardware-keyboard-arrow-left"></i> Back
  <%end%>
    <button type="submit" class="btn btn-success pull-right"><i class="mdi-content-save"></i> Save</button>
  </div>
  <br>
  <% if @semester_mark.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@semester_mark.errors.count, "error") %> prohibited this semester_mark from being saved:</h2>

      <div class="alert alert-danger">
      <% @semester_mark.errors.full_messages.each do |message| %>
        <%= message %><br/>
      <% end %>
      </div>
    </div>
  <% end %>

  <div class="form-group">
    <%= f.label :semester %><br>
    <%= f.number_field :semester,:class=>"form-control" ,:required=>true %>
  </div>
  <div class="form-group">
    <%= f.label :gpa %><br>
    <%= f.text_field :gpa,:class=>"form-control" ,:required=>true %>
  </div>
  <div class="form-group">
    <%= f.label :pass_year %><br>
    <%= f.select :pass_year, (Time.zone.now.year - 20)..(Time.zone.now.year + 1),:class=>"form-control" ,:required=>true %>
  </div>
  <div class="actions">
  </div>
<% end %>
