<p id="notice"><%= notice %></p>

<h1>Listing Semester Marks</h1>

<table>
  <thead>
    <tr>
      <th>Semester</th>
      <th>Gpa</th>
      <th>Pass year</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @semester_marks.each do |semester_mark| %>
      <tr>
        <td><%= semester_mark.semester %></td>
        <td><%= semester_mark.gpa %></td>
        <td><%= semester_mark.pass_year %></td>
        <td><%= link_to 'Show', semester_mark %></td>
        <td><%= link_to 'Edit', edit_semester_mark_path(semester_mark) %></td>
        <td><%= link_to 'Destroy', semester_mark, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Semester mark', new_semester_mark_path %>
