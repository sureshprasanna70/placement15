<div class="well col-lg-12" id="ajaxarea">
  <h1><center><i class="mdi-maps-local-library"></i>College Profile</center></h1>
  <%if @college_profiles.nil?%>
    <center><p class="message"><i class="mdi-alert-warning"></i> No College Profile yet</p></center>
    <center><a class="btn btn-primary" href="/college_profiles/new"><i class="mdi-content-add"></i> Add</a>
  </center>
    <%else%>

  </div>
  <div class="col-lg-12 well">
    <div class="col-md-6">
      <label>Level:</label>
      <%= @college_profiles.level %>
    </div>

    <div class="col-md-6">
      <label>Degree:</label>
      <%= @college_profiles.degree %>
    </div>
    <div class="col-md-6">
      <strong>Branch:</strong>
      <%= @college_profiles.branch %>
    </div>

    <div class="col-md-6">
      <strong>College:</strong>
      <%= @college_profiles.college %>
    </div>
    <div class="col-md-6">
      <strong>Semester:</strong>
      <%= @college_profiles.semester %>
    </div>

    <div class="col-md-6">
      <strong>Cgpa:</strong>
      <%= @college_profiles.cgpa %>
    </div>
    <div class="col-md-6">
      <%= link_to 'Edit', edit_college_profile_path(@college_profiles),:class=>"btn btn-large btn-success" %> 
    </div>
    <div class="col-md-6">
      <%= link_to 'Back', college_profiles_path,:class=>"btn btn-large btn-danger" %> 
    </div>
    <%end%>
